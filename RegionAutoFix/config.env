# RegionAutoFix config - shell-compatible key=value
worldsave="/path/to/7DaysToDie/Saves/WorldName/WorldName"
backup="/path/to/RegionAutoFix/Saves"
username="linuxuser"
savetime="60"
savecount="24"
logfile="/path/to/sdtdserver-console.log"
webhook=""
telnet="8081"

# Optional
startcmd=""
stopcmd=""
heartbeat_cmd="version"
telnet_password=""
# telnet_timeout: 2-5 seconds (default 5)
telnet_timeout="5"

# Testing: if true, no stop/start/kill, no overwrite; still log and optional Discord
dry_run="false"
